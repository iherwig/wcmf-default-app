{
    "name": "wcmf/wcmf-default-app",
    "type": "project",
    "description": "Default application for the wCMF web application framework",
    "keywords": ["application", "app", "cms", "template", "wcmf"],
    "homepage": "https://wcmf.wemove.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Ingo Herwig",
            "email": "ingo@wemove.com"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "olympos/chronos-generator",
                "version": "1.0.25",
                "dist": {
                    "url": "https://downloads.sourceforge.net/project/olympos/ChronosGenerator-wCMF/ChronosGenerator-wCMF-1.0.25.zip",
                    "type": "zip"
                }
            }
        },
        {
            "url": "https://github.com/iherwig/wcmf-asset-installer.git",
            "type": "git"
        },
        {
            "type": "package",
            "package": {
                "name": "dojo/dojo",
                "type": "wcmf-asset",
                "version": "1.16",
                "dist": {
                    "url": "https://github.com/dojo/dojo/archive/1.16.zip",
                    "type": "zip"
                },
                "require": {
                    "wcmf/asset-installer": "*"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "dojo/dijit",
                "type": "wcmf-asset",
                "version": "1.16",
                "dist": {
                    "url": "https://github.com/dojo/dijit/archive/1.16.zip",
                    "type": "zip"
                },
                "require": {
                    "wcmf/asset-installer": "*"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "dojo/dojox",
                "type": "wcmf-asset",
                "version": "1.16",
                "dist": {
                    "url": "https://github.com/dojo/dojox/archive/1.16.zip",
                    "type": "zip"
                },
                "require": {
                    "wcmf/asset-installer": "*"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "dojo/util",
                "type": "wcmf-asset",
                "version": "1.16",
                "dist": {
                    "url": "https://github.com/dojo/util/archive/1.16.zip",
                    "type": "zip"
                },
                "require": {
                    "wcmf/asset-installer": "*"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "sitepen/dstore",
                "type": "wcmf-asset",
                "version": "1.2.1",
                "dist": {
                    "url": "https://github.com/SitePen/dstore/archive/v1.2.1.zip",
                    "type": "zip"
                },
                "require": {
                    "wcmf/asset-installer": "*"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "sitepen/dgrid",
                "type": "wcmf-asset",
                "version": "1.3.0",
                "dist": {
                    "url": "https://github.com/SitePen/dgrid/archive/v1.3.0.zip",
                    "type": "zip"
                },
                "require": {
                    "wcmf/asset-installer": "*"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "ckeditor/ckeditor",
                "type": "wcmf-asset",
                "version": "4.14.0",
                "dist": {
                    "url": "https://github.com/ckeditor/ckeditor-releases/archive/full/4.14.0.zip",
                    "type": "zip"
                },
                "require": {
                    "wcmf/asset-installer": "*"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "studio-42/elfinder-js",
                "type": "wcmf-asset",
                "version": "2.1.55",
                "dist": {
                    "url": "https://github.com/Studio-42/elFinder/archive/2.1.55.zip",
                    "type": "zip"
                },
                "require": {
                    "wcmf/asset-installer": "*"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "sirprize/dojomat",
                "type": "wcmf-asset",
                "version": "dev-master",
                "source": {
                    "url": "https://github.com/sirprize/dojomat.git",
                    "type": "git",
                    "reference": "master"
                },
                "require": {
                    "wcmf/asset-installer": "*"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "sirprize/routed",
                "type": "wcmf-asset",
                "version": "dev-master",
                "source": {
                    "url": "https://github.com/sirprize/routed.git",
                    "type": "git",
                    "reference": "master"
                },
                "require": {
                    "wcmf/asset-installer": "*"
                }
            }
        }
    ],
    "minimum-stability": "RC",
    "require": {
        "php": ">=7.0",
        "wcmf/asset-installer": "~1.0",
        "wcmf/wcmf": "~4.1",
        "dojo/dojo": "~1.16",
        "dojo/dijit": "~1.16",
        "dojo/dojox": "~1.16",
        "dojo/util": "~1.16",
        "sitepen/dstore": "~1.2",
        "sitepen/dgrid": "~1.3",
        "oomphinc/composer-installers-extender": "*",
        "components/bootstrap": "~3.4",
        "components/font-awesome": "~4.7",
        "ckeditor/ckeditor": "~4.14",
        "components/jquery": "~3.4.0",
        "components/jqueryui": "~1.12",
        "studio-42/elfinder-js": "~2.1",
        "sirprize/dojomat": "dev-master",
        "sirprize/routed": "dev-master",
        "redeyeventures/geopattern": "~1.1",
        "gumlet/php-image-resize": "~1.9",
        "studio-42/elfinder": "~2.1",
        "smarty/smarty": "~3.1",
        "kriswallsmith/assetic": "~1.4",
        "toopay/assetic-minifier": "~1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7",
        "phpunit/dbunit": "~1.4",
        "php-webdriver/webdriver": "~1.6"
    },
    "extra": {
      "installer-types": ["component"],
      "installer-paths": {
        "app/public/vendor/{$name}/": ["type:component"]
      }
    }
}
