    "minimum-stability": "RC",
    "require": {
        "php": ">=8.1",
        "wcmf/asset-installer": "~2.0",
        "wcmf/wcmf": "~4.1",
        "dojo/dojo": "~1.17",
        "dojo/dijit": "~1.17",
        "dojo/dojox": "~1.17",
        "dojo/util": "~1.17",
        "sitepen/dstore": "~1.2",
        "sitepen/dgrid": "~1.3",
        "oomphinc/composer-installers-extender": "*",
        "components/bootstrap": "~3.4",
        "components/font-awesome": "~4.7",
        "ckeditor/ckeditor": "~4.22",
        "sa-si-dev/virtual-select": "~1.0",
        "components/jquery": "~3.6.0",
        "components/jqueryui": "~1.12",
        "studio-42/elfinder-js": "~2.1",
        "sirprize/dojomat": "dev-master",
        "sirprize/routed": "dev-master",
        "redeyeventures/geopattern": "~1.1",
        "intervention/image": "~3.6",
        "studio-42/elfinder": "~2.1",
        "smarty/smarty": "~5",
        "kriswallsmith/assetic": "~1.4",
        "toopay/assetic-minifier": "~1.3"
    },
    "require-dev": {
    },
    "extra": {
      "installer-types": ["component"],
      "installer-paths": {
        "app/public/vendor/{$name}/": ["type:component"]
      }
    },
    "config": {
        "allow-plugins": {
            "wcmf/asset-installer": true,
            "composer/installers": true,
            "oomphinc/composer-installers-extender": true
        }
    }
}
